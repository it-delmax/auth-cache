# Auth Cache Package Configuration
AUTH_CACHE_DB_CONNECTION=etg_utf8

# Cache TTL Settings (in seconds)
AUTH_CACHE_USER_TTL=21600       # 6 hours
AUTH_CACHE_TOKEN_TTL=43200      # 12 hours
AUTH_CACHE_API_ACCESS_TTL=86400 # 24 hours
AUTH_CACHE_API_SLUG_TTL=86400   # 24 hours

# Cache Key Prefixes
AUTH_CACHE_USER_PREFIX=user:
AUTH_CACHE_TOKEN_PREFIX=token:
AUTH_CACHE_API_ACCESS_PREFIX=api_access:
AUTH_CACHE_API_SLUG_PREFIX=api_slug:

# Database Connections
AUTH_CACHE_USER_CONNECTION=etg_utf8
AUTH_CACHE_TOKEN_CONNECTION=mysql
AUTH_CACHE_API_CONNECTION=etg_utf8

# Table Names
AUTH_CACHE_USERS_TABLE=USERS_VIEW
AUTH_CACHE_TOKENS_TABLE=personal_access_tokens
AUTH_CACHE_APIS_TABLE=DMX_API
AUTH_CACHE_API_USERS_TABLE=DMX_API_USERS

# Cache Warming
AUTH_CACHE_WARMING_ENABLED=true
AUTH_CACHE_WARMING_CHUNK_SIZE=100
AUTH_CACHE_WARMING_QUEUE=cache

# Cache Invalidation
AUTH_CACHE_INVALIDATION_ENABLED=true
AUTH_CACHE_INVALIDATION_SCHEDULE=hourly
AUTH_CACHE_INVALIDATION_QUEUE=cache

# Logging
AUTH_CACHE_LOGGING_ENABLED=true
AUTH_CACHE_LOG_LEVEL=info
# AUTH_CACHE_LOG_CHANNEL=null

# Performance
AUTH_CACHE_SERIALIZE=true
AUTH_CACHE_COMPRESS=false
AUTH_CACHE_FALLBACK_DB=true
